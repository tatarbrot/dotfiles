# set wmname
wmname LG3D
export _JAVA_AWT_WM_NONREPARENTING=1

# do some autostarts
iceweasel &
thunderbird &
udiskie &
pulseaudio --start &
statnot $HOME/.config/statnot/config.py &
pomo.sh notify &

# set keyboard layout to Swiss German
#setxkbmap ch -option apple:badmap
setxkbmap ch

# Run dwm
exec dwm
